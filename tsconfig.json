{
  "include": [
    "next-env.d.ts",
    "types",
    "src",
    "pages",
    "data"
  ],
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "lib": ["es2020", "dom"],
    "jsx": "preserve",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "noErrorTruncation": false,
    "allowJs": true,
    "isolatedModules": true,
    /* files are emitted by babel */
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": [
      "react"
    ],
    "incremental": true,
    "paths": {
      "@mui/markdown": ["./packages/markdown"],
      "@mui/markdown/*": ["./packages/markdown/*"],
    }
  },
  "exclude": [
    "node_modules",
    "export"
  ]
}
